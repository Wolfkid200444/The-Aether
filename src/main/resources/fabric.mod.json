{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contributors": [
    "${mod_credits}"
  ],
  "contact": {
    "sources": "${mod_url}",
    "issues": "${mod_tracker}"
  },
  "license": "${mod_license}",
  "icon": "${mod_name}.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.aetherteam.aether.Aether"
    ],
    "client": [
      "com.aetherteam.aether.client.AetherClient"
    ],
    "fabric-datagen": [
      "com.aetherteam.aether.data.AetherData"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "neoforge.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabric_loader_version_range}",
    "minecraft": "${minecraft_version_range}",
    "java": ">=21",
    "fabric-api": "${fabric_version_range}"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_6880": [
        "net/neoforged/neoforge/common/extensions/IHolderExtension"
      ],
      "net/minecraft/class_2370": [
        "net/neoforged/neoforge/common/extensions/IMappedRegistryExtension<TT;>"
      ],
      "net/minecraft/class_1937": [
        "net/fabricmc/fabric/api/attachment/v1/AttachmentTarget"
      ]
    }
  }
}